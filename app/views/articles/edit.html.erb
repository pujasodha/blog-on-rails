<h3>Edit Exisiting Article: <%= @article.title %></h3>


<% if @article.errors.any? %>
<ul>
    <% @article.errors.full_messages.each do |msg| %>
    <l1><%= msg %></l1>
</ul>
<% end %>
<% end %>


<%=form_for @article do |f| %>

<p>
<%= f.label :title%> <br>
<%= f.text_field :title%>
</p>

<p>
<%= f.label :description%> <br>
<%= f.text_area :description%>
</p>

<%= f.submit %>
<% end %>